{
  "version": 2,
  "buildCommand": "cp -r ../packages/db ./packages/db && bun install && bun run db:generate && bun run build",
  "installCommand": "bun install",
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ],
  "env": {
    "PRISMA_SCHEMA_DISABLE_ADVISORY_LOCK": "1"
  }
}
